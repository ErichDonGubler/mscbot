{{outlet}}

<h3><a id="ci-failures"/>CI Failures</h3>

<h5>Failures in the last 24 hours</h5>

<table class="u-full-width">
    <thead>
        <tr>
            <th>Build Number</th>
            <th>Builder</th>
            <th>Message</th>
            <th>Start Time</th>
            <th>End Time</th>
        </tr>
    </thead>
    <tbody>
        {{#each model.recent_failures as |build|}}

        <tr>
            <td><a href="{{build.url}}" target="_blank">{{build.build_id}} -- {{ build.job_id }}</a></td>
            <td>{{build.builder_name}} -- {{ build.env }} </td>
            <td>{{build.message}}</td>
            <td>{{build.start_time}}</td>
            <td>{{build.end_time}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>

{{scatter-plot title='Number of Failed CI Builds (Linux)' data=model.linux.per_builder_fails}}

{{scatter-plot title='Number of Failed CI Builds (Mac)' data=model.mac.per_builder_fails}}

{{scatter-plot title='Number of Failed CI Builds (Windows)' data=model.windows.per_builder_fails}}

<h3><a id="ci-times"/>CI Times</h3>

{{line-chart title='Times of Successful CI Builds (Linux)' data=model.linux.per_builder_times}}

{{line-chart title='Times of Successful CI Builds (Mac)' data=model.mac.per_builder_times}}

{{line-chart title='Times of Successful CI Builds (Windows)' data=model.windows.per_builder_times}}

<hr/>
